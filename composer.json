{
    "name": "kly/laravel-onesdk",
    "description": "Laravel service provide for onesdk app php SDK",
    "type": "library",
    "require": {
        "karsa/onesdk": "dev-master",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/laravel": "^8.0",
        "orchestra/testbench": "^6.0",
        "symplify/easy-coding-standard": "^9.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Charis",
            "email": "me@chars.me"
        },
        {
            "name": "Denta Zamzam Yasin Muhajir",
            "email": "denta.muhajir@kly.id",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit --bootstrap ./test/bootstrap.php ./test/",
        "cs-check": "ecs check src test",
        "cs-fix": "ecs check src test --fix"
    },
    "autoload": {
        "psr-4": {
            "One\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "One\\Provider\\Test\\": "test/"
        }
    },
    "repositories": [
        {"name": "karsa/onesdk", "type": "vcs", "url": "https://github.com/karsakly/onesdk.git"}
    ]
}
